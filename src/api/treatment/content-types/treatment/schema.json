{
  "kind": "collectionType",
  "collectionName": "treatments",
  "info": {
    "singularName": "treatment",
    "pluralName": "treatments",
    "displayName": "Treatments"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "excerpt": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "duration": {
      "type": "time"
    },
    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::form-submission.form-submission",
      "mappedBy": "treatment"
    },
    "bookingUrl": {
      "type": "string"
    },
    "composites": {
      "type": "dynamiczone",
      "components": [
        "composites.testimonials",
        "composites.generic-content",
        "composites.cta-section",
        "composites.contact-form",
        "composites.card-display",
        "composites.accordions"
      ]
    }
  }
}
